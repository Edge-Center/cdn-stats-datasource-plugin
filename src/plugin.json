{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "cdn-stats",
  "id": "gcorelabs-cdn-stats-datasource",
  "metrics": true,
  "info": {
    "description": "",
    "author": {
      "name": "Ilya Vishneuski",
      "url": "https://gcorelabs.com/"
    },
    "keywords": ["cdn", "statistics"],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://github.com/grafana/grafana-starter-datasource"
      },
      {
        "name": "License",
        "url": "https://github.com/grafana/grafana-starter-datasource/blob/master/LICENSE"
      }
    ],
    "screenshots": [],
    "version": "0.0.4",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=7.0.0",
    "plugins": []
  },
  "routes": [
    {
      "path": "statistics/aggregate/stats",
      "url": "https://api.gcdn.co/statistics/aggregate/stats",
      "headers": [
        {
          "name": "Authorization",
          "content": "{{ .SecureJsonData.apiKey }}"
        }
      ]
    },
    {
      "path": "resources",
      "url": "https://api.gcdn.co/resources",
      "headers": [
        {
          "name": "Authorization",
          "content": "{{ .SecureJsonData.apiKey }}"
        }
      ]
    },
    {
      "path": "users/me",
      "url": "https://api.gcdn.co/users/me",
      "headers": [
        {
          "name": "Authorization",
          "content": "{{ .SecureJsonData.apiKey }}"
        }
      ]
    }
  ]
}
